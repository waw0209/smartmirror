<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="300dip"
    android:layout_height="200dip" >

    <Button
        android:id="@+id/btnConfirm"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/btnCancel1"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@+id/btnCancel1"
        android:text="확인" />

    <Button
        android:id="@+id/btnCancel1"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:text="설정변경" />

    <TextView
        android:text="미러 KEY     ="
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:id="@+id/textView4"
        android:layout_alignBottom="@+id/keycheak"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="31dp"
        android:layout_marginStart="31dp"
        android:layout_alignTop="@+id/keycheak" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/keycheak"
        android:textSize="20sp"
        android:layout_marginTop="43dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/btnCancel1"
        android:layout_toEndOf="@+id/btnCancel1" />

    <TextView
        android:text="사용자 이름   ="
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4"
        android:id="@+id/textView5"
        android:textSize="20sp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewname"
        android:textSize="20sp"
        android:layout_marginLeft="10dp"
        android:layout_alignBaseline="@+id/textView5"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignLeft="@+id/keycheak"
        android:layout_alignStart="@+id/keycheak" />


</RelativeLayout>
